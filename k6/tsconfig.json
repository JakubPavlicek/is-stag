{
  "compilerOptions": {
    "target": "ES2024",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "allowImportingTsExtensions": true,
    "lib": ["ES2024"],
    "types": ["k6"],
    "paths": {
